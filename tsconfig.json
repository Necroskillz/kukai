{
  "compileOnSave": false,
  "compilerOptions": {
    "downlevelIteration": true,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "allowJs": false,
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "module": "esnext",
    "baseUrl": "./",
    "paths": {
      "stream": [
        "./node_modules/stream-browserify/index"
      ],
      "http": [
        "./src/empty"
      ],
      "https": [
        "./src/empty"
      ],
      "path": [
        "./src/empty"
      ],
      "crypto": [
        "./src/empty"
      ],
      "os": [
        "./src/empty"
      ],
      "fs": [
        "./src/empty"
      ],
    },
  }
}